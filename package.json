{
  "name": "mktemp",
  "description": "mktemp command for node.js",
  "version": "1.0.1",
  "author": "sasa+1 <sasaplus1@gmail.com>",
  "contributors": [
    "Michael Ficarra <github.public.email@michael.ficarra.me>",
    "rjz <rj@rjzaworski.com>"
  ],
  "devDependencies": {
    "@tsconfig/strictest": "2.0.5",
    "@types/node": "22.15.31",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "eslint": "8.39.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-tsdoc": "0.2.17",
    "fixpack": "4.0.0",
    "npm-run-all2": "6.2.6",
    "prettier": "2.8.8",
    "typescript": "5.8.3",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "20",
    "npm": "10"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "optionalDependencies": {
    "@rollup/rollup-darwin-arm64": "4.43.0",
    "@rollup/rollup-linux-x64-gnu": "4.43.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "parser": "json-stringify"
        }
      }
    ]
  },
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/sasaplus1/mktemp.git"
  },
  "scripts": {
    "build": "run-p --print-label build:*",
    "build:cjs": "tsc -p .tsconfigs/cjs.json",
    "build:esm": "tsc -p .tsconfigs/esm.json",
    "fix": "run-p --print-label fix:*",
    "fix:eslint": "eslint --fix .",
    "fix:fixpack": "fixpack",
    "fix:prettier": "prettier --write .",
    "lint": "run-p --print-label lint:*",
    "lint:eslint": "eslint .",
    "lint:fixpack": "fixpack --dryRun",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc --noEmit",
    "test": "vitest run"
  },
  "sideEffects": false,
  "types": "./dist/cjs/index.js"
}
